<%= form_tag students_path do %>
  <%= label_tag(:full_name, "Full name") %>
  <%= text_field_tag(:full_name) %>

  <%= label_tag(:superhero_identity, "Superhero Identity") %>
  <%= text_field_tag(:superhero_identity) %>

  <%= label_tag(:power, "Power") %>
  <%= text_field_tag(:power) %>

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>

  <%= submit_tag("Create Superstudent") %>
<% end %>